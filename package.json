{
  "name": "@fe6/icon-monorepo",
  "private": true,
  "scripts": {
    "preinstall": "node scripts/preinstall && husky install",
    "gen:json": "ts-node scripts/build-icons.ts",
    "gen:code": "ts-node scripts/build-code.ts",
    "gen:clean": "rm -rf packages/*/src/runtime packages/*/src/index.ts packages/*/src/icons",
    "gen": "pnpm gen:clean && pnpm gen:json && pnpm gen:code",
    "lint:fix": "eslint './**/*.{js,ts,tsx,json}' --fix && pretty-quick",
    "lint": "eslint './**/*.{js,ts,tsx,json}' && pretty-quick"
  },
  "engines": {
    "node": ">= 16.0.0",
    "pnpm": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "node_modules/@fe6/norm/src/.cz-configrc.js"
    }
  },
  "packageManager": "pnpm",
  "peerDependencies": {
    "vue": "3.x"
  },
  "devDependencies": {
    "@antfu/ni": "^0.10.0",
    "@fe6/norm": "^3.4.3",
    "@types/css-tree": "^1.0.6",
    "@types/mkdirp": "^1.0.2",
    "@types/tinycolor2": "^1.4.3",
    "@vue/runtime-dom": "^3.2.20",
    "chalk": "^4.1.2",
    "css-tree": "^1.1.3",
    "csv-parse": "^4.16.3",
    "csv-stringify": "^5.6.5",
    "cz-customizable": "^6.3.0",
    "husky": "^7.0.2",
    "mkdirp": "^1.0.4",
    "pretty-quick": "^3.1.1",
    "tinycolor2": "^1.4.2",
    "ts-node": "^10.3.0",
    "xml-js": "^1.6.11"
  },
  "pnpm": {
    "overrides": {
      "@fe6/icon-vue": "workspace:^"
    }
  }
}
